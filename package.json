{
  "name": "react-app-seed",
  "version": "1.0.0",
  "description": "React App Seed",
  "config": {
    "csp": {
      "dev": "script-src * 'unsafe-inline'; style-src 'self' 'unsafe-inline'",
      "prod": "script-src 'self'; style-src 'self' 'unsafe-inline'; connect-src 'self' https://login.microsoftonline.com"
    },
    "alias": {
      "c1": {
        "hello/Hello$": "hello1/Hello"
      },
      "c2": {
        "hello": "hello2"
      }
    },
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 8000
  },
  "scripts": {
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "webpack": "webpack --colors --display-error-details --progress --config ./webpack/config",
    "start": "NODE_ENV=dev MODE=c1 node ./webpack/dev-server",
    "startAdmin": "NODE_ENV=dev MODE=c2 node ./webpack/dev-server",
    "prebuild": "npm run clean",
    "build": "npm run webpack"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "node-libs-browser": "^0.5.3",
    "opn": "^3.0.3",
    "rimraf": "^2.4.4",
    "template-html-loader": "0.0.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "babel-runtime": "^6.2.0",
    "history": "^1.13.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0"
  }
}
